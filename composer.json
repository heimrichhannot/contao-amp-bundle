{
  "name": "heimrichhannot/contao-amp-bundle",
  "type": "contao-bundle",
  "description": "This bundle offers functionality concerning \"Accelerated Mobile Pages\" (AMP) for the Contao CMS.",
  "license": "LGPL-3.0-or-later",
  "require": {
    "php": "^7.4 || ^8.0",
    "contao/core-bundle": "^4.9",
    "heimrichhannot/contao-head-bundle": "^1.7",
    "heimrichhannot/contao-twig-support-bundle": "^1.5",
    "heimrichhannot/contao-utils-bundle": "^2.7",
    "symfony/event-dispatcher-contracts": "^1.0 || ^2.0 || ^3.0",
    "symfony/http-foundation": "^4.4 || ^5.4"
  },
  "suggest": {
    "heimrichhannot/contao-encore-bundle": "^1.4.1"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\AmpBundle\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\AmpBundle\\ContaoManager\\Plugin",
    "foxy": true
  }
}
